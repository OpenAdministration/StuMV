{
    "name": "open-administration/yii2-new-app-template",
    "description": "Template for new Yii2 Applications",
    "license": "tba",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Lukas Staab",
            "email": "lukas.staab@open-administration.de"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "yiisoft/yii2": "~2.0",
        "yiisoft/yii2-swiftmailer": "~2.1.0",
        "yiisoft/yii2-bootstrap4": "~2.0",
        "yiisoft/yii2-bootstrap": "~2.0",
        "daxslab/yii2-taggedview": "dev-master",
        "kartik-v/yii2-helpers": "^1.3",
        "rmrevin/yii2-fontawesome": "3.7.0",
        "jasig/phpcas": "~1.3",
        "monolog/monolog": "~2.2.0",
        "ext-bcmath": "*",
        "ext-mbstring": "*",
        "ext-ctype": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.1.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "5.0.x-dev",
        "codeception/specify": "dev-master",
        "codeception/verify": "dev-master",
        "insolita/yii2-migration-generator": "~3.1"
    },
    "scripts": {
        "post-install-cmd": [
            "yii\\composer\\Installer::postInstall"
        ],
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject",
            "yii\\composer\\Installer::postInstall"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
